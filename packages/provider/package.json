    "@y-kafka-collabation-server/rollup-config": "workspace:*",
    "eslint": "^9.31.0",
    "typescript": "5.8.2",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@y-kafka-collabation-server/protocol": "workspace:*",
    "@y/protocols": "^1.0.6-3",
    "@y/y": "^14.0.0-16"
  }
